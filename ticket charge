#include <stdio.h>
int main()
 {
    int age;
    printf("enter your age: ");
    scanf("%d", &age);
    if (age >= 55) 
	{
        printf("ticket charge: $10.00\n");
    } 
	else if (age >= 21 && age <= 54)
	 {
        printf("ticket charge: $15.00\n");
    } 
	else if (age >= 13 && age <= 20)
	 {
        printf("ticket charge: $10.00\n");
    } 
	else if (age >= 3 && age <= 12)
	 {
        printf("ticket charge: $5.00\n");
    } 
	else
	 {
        printf("ticket charge: free\n");
    }

    return 0;
}
